{
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": false
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-transform-runtime",
		"@babel/plugin-transform-async-to-generator",
		"babel-plugin-styled-components",
		"inline-react-svg",
		[
			"module-resolver",
			{
				"alias": {
					"@": "./src",
					"~": "./src/components"
				}
			}
		]
	]
}
